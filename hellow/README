# for interactive run, setup cpus/threads to be 4, type
[ ...$] export OMP_NUM_THREADS=4

# compile
 gcc –fopenmp –o hellow_omp hellow_omp.c
	
 gfortran –fopenmp –o hellow_omp hellow_omp.f90

# interactive run
 ./hellow_omp

# slurm submit as a job (on cluster only, not on Magic Castle account)
 sbatch hellow_omp.sh

